{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "release$published",
  "type": "object",
  "properties": {
    "action": {
      "type": "string",
      "enum": [
        "published"
      ]
    },
    "installation": {
      "$ref": "common/installation-lite.schema.json"
    },
    "organization": {
      "$ref": "common/organization.schema.json"
    },
    "release": {
      "allOf": [
        {
          "$ref": "common/release.schema.json"
        },
        {
          "type": "object",
          "properties": {
            "assets": {
              "type": "array"
            },
            "assets_url": {
              "type": "string"
            },
            "author": {
              "type": "object",
              "properties": {
                "avatar_url": {
                  "type": "string"
                },
                "events_url": {
                  "type": "string"
                },
                "followers_url": {
                  "type": "string"
                },
                "following_url": {
                  "type": "string"
                },
                "gists_url": {
                  "type": "string"
                },
                "gravatar_id": {
                  "type": "string"
                },
                "html_url": {
                  "type": "string"
                },
                "id": {
                  "type": "integer"
                },
                "login": {
                  "type": "string"
                },
                "node_id": {
                  "type": "string"
                },
                "organizations_url": {
                  "type": "string"
                },
                "received_events_url": {
                  "type": "string"
                },
                "repos_url": {
                  "type": "string"
                },
                "site_admin": {
                  "type": "boolean"
                },
                "starred_url": {
                  "type": "string"
                },
                "subscriptions_url": {
                  "type": "string"
                },
                "type": {
                  "type": "string"
                },
                "url": {
                  "type": "string"
                }
              }
            },
            "body": {
              "type": [
                "string",
                "null"
              ]
            },
            "created_at": {
              "type": "string"
            },
            "draft": {
              "type": "boolean"
            },
            "html_url": {
              "type": "string"
            },
            "id": {
              "type": "integer"
            },
            "name": {
              "type": "string"
            },
            "node_id": {
              "type": "string"
            },
            "prerelease": {
              "type": "boolean"
            },
            "published_at": {
              "type": "string",
              "format": "date-time"
            },
            "tag_name": {
              "type": "string"
            },
            "tarball_url": {
              "type": "string"
            },
            "target_commitish": {
              "type": "string"
            },
            "upload_url": {
              "type": "string"
            },
            "url": {
              "type": "string"
            },
            "zipball_url": {
              "type": "string"
            }
          },
          "required": [
            "published_at"
          ]
        }
      ]
    },
    "repository": {
      "$ref": "common/repository.schema.json"
    },
    "sender": {
      "$ref": "common/user.schema.json"
    }
  },
  "required": [
    "action",
    "release",
    "repository"
  ],
  "additionalProperties": false,
  "title": "release published event"
}