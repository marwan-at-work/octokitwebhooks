{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "project_card$moved",
  "type": "object",
  "properties": {
    "action": {
      "type": "string",
      "enum": [
        "moved"
      ]
    },
    "changes": {
      "type": "object",
      "properties": {
        "column_id": {
          "type": "object",
          "properties": {
            "from": {
              "type": "integer"
            }
          },
          "required": [
            "from"
          ],
          "additionalProperties": false
        }
      },
      "required": [
        "column_id"
      ],
      "additionalProperties": false
    },
    "installation": {
      "$ref": "common/installation-lite.schema.json"
    },
    "organization": {
      "$ref": "common/organization.schema.json"
    },
    "project_card": {
      "allOf": [
        {
          "$ref": "common/project-card.schema.json"
        },
        {
          "type": "object",
          "properties": {
            "after_id": {
              "type": [
                "number",
                "null"
              ]
            }
          },
          "required": [
            "after_id"
          ]
        }
      ]
    },
    "repository": {
      "$ref": "common/repository.schema.json"
    },
    "sender": {
      "$ref": "common/user.schema.json"
    }
  },
  "required": [
    "action",
    "project_card",
    "sender"
  ],
  "additionalProperties": false,
  "title": "project_card moved event"
}