{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "meta$deleted",
  "type": "object",
  "properties": {
    "action": {
      "type": "string",
      "enum": [
        "deleted"
      ]
    },
    "hook": {
      "type": "object",
      "properties": {
        "active": {
          "type": "boolean"
        },
        "config": {
          "type": "object",
          "properties": {
            "content_type": {
              "type": "string",
              "enum": [
                "json",
                "form"
              ]
            },
            "insecure_ssl": {
              "type": "string"
            },
            "secret": {
              "type": "string"
            },
            "url": {
              "type": "string",
              "format": "uri"
            }
          },
          "required": [
            "content_type",
            "insecure_ssl",
            "url"
          ],
          "additionalProperties": false
        },
        "created_at": {
          "type": "string"
        },
        "events": {
          "$ref": "common/webhook-events.schema.json"
        },
        "id": {
          "type": "integer"
        },
        "name": {
          "type": "string"
        },
        "type": {
          "type": "string"
        },
        "updated_at": {
          "type": "string"
        }
      },
      "required": [
        "type",
        "id",
        "name",
        "active",
        "events",
        "config",
        "updated_at",
        "created_at"
      ],
      "additionalProperties": false,
      "description": "The modified webhook. This will contain different keys based on the type of webhook it is: repository, organization, business, app, or GitHub Marketplace."
    },
    "hook_id": {
      "type": "integer",
      "description": "The id of the modified webhook."
    },
    "repository": {
      "$ref": "common/repository.schema.json"
    },
    "sender": {
      "$ref": "common/user.schema.json"
    }
  },
  "required": [
    "action",
    "hook_id",
    "hook",
    "repository"
  ],
  "additionalProperties": false,
  "title": "meta deleted event"
}