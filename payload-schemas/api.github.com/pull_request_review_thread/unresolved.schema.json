{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "pull_request_review_thread$unresolved",
  "type": "object",
  "properties": {
    "action": {
      "type": "string",
      "enum": [
        "unresolved"
      ]
    },
    "installation": {
      "$ref": "common/installation-lite.schema.json"
    },
    "organization": {
      "$ref": "common/organization.schema.json"
    },
    "pull_request": {
      "$ref": "common/simple-pull-request.schema.json"
    },
    "repository": {
      "$ref": "common/repository.schema.json"
    },
    "sender": {
      "$ref": "common/user.schema.json"
    },
    "thread": {
      "type": "object",
      "properties": {
        "comments": {
          "type": "array",
          "items": {
            "$ref": "common/pull-request-review-comment.schema.json"
          }
        },
        "node_id": {
          "type": "string"
        }
      },
      "required": [
        "node_id",
        "comments"
      ],
      "additionalProperties": false
    }
  },
  "required": [
    "action",
    "thread",
    "pull_request",
    "repository"
  ],
  "additionalProperties": false,
  "title": "pull_request_review_thread unresolved event"
}