{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "workflow_job$in_progress",
  "type": "object",
  "properties": {
    "action": {
      "type": "string",
      "enum": [
        "in_progress"
      ]
    },
    "enterprise": {
      "$ref": "common/enterprise.schema.json"
    },
    "installation": {
      "$ref": "common/installation-lite.schema.json"
    },
    "organization": {
      "$ref": "common/organization.schema.json"
    },
    "repository": {
      "$ref": "common/repository.schema.json"
    },
    "sender": {
      "$ref": "common/user.schema.json"
    },
    "workflow_job": {
      "allOf": [
        {
          "$ref": "common/workflow-job.schema.json"
        },
        {
          "type": "object",
          "properties": {
            "check_run_url": {
              "type": "string"
            },
            "completed_at": {
              "type": "null"
            },
            "conclusion": {
              "type": "null"
            },
            "head_sha": {
              "type": "string"
            },
            "html_url": {
              "type": "string"
            },
            "id": {
              "type": "integer"
            },
            "labels": {
              "type": "array",
              "items": {
                "type": "string"
              }
            },
            "name": {
              "type": "string"
            },
            "node_id": {
              "type": "string"
            },
            "run_attempt": {
              "type": "integer"
            },
            "run_id": {
              "type": "integer"
            },
            "run_url": {
              "type": "string"
            },
            "runner_group_id": {
              "type": "null"
            },
            "runner_group_name": {
              "type": "null"
            },
            "runner_id": {
              "type": "null"
            },
            "runner_name": {
              "type": "null"
            },
            "started_at": {
              "type": "string"
            },
            "status": {
              "type": "string",
              "enum": [
                "in_progress"
              ]
            },
            "steps": {
              "type": "array",
              "items": {
                "$ref": "common/workflow-step-in_progress.schema.json"
              }
            },
            "url": {
              "type": "string"
            }
          },
          "required": [
            "status",
            "steps"
          ]
        }
      ]
    }
  },
  "required": [
    "action",
    "repository",
    "sender",
    "workflow_job"
  ],
  "additionalProperties": false,
  "title": "workflow_job in_progress event"
}