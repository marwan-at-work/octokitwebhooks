{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "check_run$rerequested",
  "type": "object",
  "properties": {
    "action": {
      "type": "string",
      "enum": [
        "rerequested"
      ]
    },
    "check_run": {
      "type": "object",
      "properties": {
        "app": {
          "$ref": "common/app.schema.json"
        },
        "check_suite": {
          "type": "object",
          "properties": {
            "after": {
              "type": [
                "string",
                "null"
              ]
            },
            "app": {
              "$ref": "common/app.schema.json"
            },
            "before": {
              "type": [
                "string",
                "null"
              ]
            },
            "conclusion": {
              "type": [
                "string",
                "null"
              ],
              "enum": [
                "success",
                "failure",
                "neutral",
                "cancelled",
                "timed_out",
                "action_required",
                "stale"
              ]
            },
            "created_at": {
              "type": "string",
              "format": "date-time"
            },
            "deployment": {
              "$ref": "common/check-run-deployment.schema.json"
            },
            "head_branch": {
              "type": [
                "string",
                "null"
              ]
            },
            "head_sha": {
              "type": "string",
              "description": "The SHA of the head commit that is being checked."
            },
            "id": {
              "type": "integer",
              "description": "The id of the check suite that this check run is part of."
            },
            "node_id": {
              "type": "string"
            },
            "pull_requests": {
              "type": "array",
              "description": "An array of pull requests that match this check suite. A pull request matches a check suite if they have the same `head_sha` and `head_branch`. When the check suite's `head_branch` is in a forked repository it will be `null` and the `pull_requests` array will be empty.",
              "items": {
                "$ref": "common/check-run-pull-request.schema.json"
              }
            },
            "status": {
              "type": "string",
              "enum": [
                "completed",
                "queued"
              ]
            },
            "updated_at": {
              "type": "string",
              "format": "date-time"
            },
            "url": {
              "type": "string",
              "format": "uri"
            }
          },
          "required": [
            "id",
            "head_branch",
            "head_sha",
            "status",
            "conclusion",
            "url",
            "before",
            "after",
            "pull_requests",
            "app",
            "created_at",
            "updated_at"
          ],
          "additionalProperties": false
        },
        "completed_at": {
          "type": [
            "string",
            "null"
          ],
          "description": "The time the check completed. This is a timestamp in [ISO 8601](https://en.wikipedia.org/wiki/ISO_8601) format: `YYYY-MM-DDTHH:MM:SSZ`."
        },
        "conclusion": {
          "type": [
            "string",
            "null"
          ],
          "enum": [
            "success",
            "failure",
            "neutral",
            "cancelled",
            "timed_out",
            "action_required",
            "stale",
            "skipped",
            null
          ],
          "description": "The result of the completed check run. Can be one of `success`, `failure`, `neutral`, `cancelled`, `timed_out`, `action_required` or `stale`. This value will be `null` until the check run has `completed`."
        },
        "deployment": {
          "$ref": "common/check-run-deployment.schema.json"
        },
        "details_url": {
          "type": "string",
          "format": "uri"
        },
        "external_id": {
          "type": "string"
        },
        "head_sha": {
          "type": "string",
          "description": "The SHA of the commit that is being checked."
        },
        "html_url": {
          "type": "string",
          "format": "uri"
        },
        "id": {
          "type": "integer",
          "description": "The id of the check."
        },
        "name": {
          "type": "string",
          "description": "The name of the check."
        },
        "node_id": {
          "type": "string"
        },
        "output": {
          "type": "object",
          "properties": {
            "annotations_count": {
              "type": "integer"
            },
            "annotations_url": {
              "type": "string",
              "format": "uri"
            },
            "summary": {
              "type": [
                "string",
                "null"
              ]
            },
            "text": {
              "type": [
                "string",
                "null"
              ]
            },
            "title": {
              "type": [
                "string",
                "null"
              ]
            }
          },
          "required": [
            "summary",
            "text",
            "annotations_count",
            "annotations_url"
          ],
          "additionalProperties": false
        },
        "pull_requests": {
          "type": "array",
          "items": {
            "$ref": "common/check-run-pull-request.schema.json"
          }
        },
        "started_at": {
          "type": "string",
          "description": "The time that the check run began. This is a timestamp in [ISO 8601](https://en.wikipedia.org/wiki/ISO_8601) format: `YYYY-MM-DDTHH:MM:SSZ`."
        },
        "status": {
          "type": "string",
          "enum": [
            "completed",
            "queued"
          ],
          "description": "The phase of the lifecycle that the check is currently in."
        },
        "url": {
          "type": "string",
          "format": "uri"
        }
      },
      "required": [
        "id",
        "head_sha",
        "external_id",
        "url",
        "html_url",
        "status",
        "conclusion",
        "started_at",
        "completed_at",
        "output",
        "name",
        "check_suite",
        "app",
        "pull_requests"
      ],
      "additionalProperties": false,
      "description": "The [check_run](https://docs.github.com/en/rest/reference/checks#get-a-check-run)."
    },
    "installation": {
      "$ref": "common/installation-lite.schema.json"
    },
    "organization": {
      "$ref": "common/organization.schema.json"
    },
    "repository": {
      "$ref": "common/repository.schema.json"
    },
    "requested_action": {
      "type": [
        "object",
        "null"
      ],
      "properties": {
        "identifier": {
          "type": "string",
          "description": "The integrator reference of the action requested by the user."
        }
      },
      "additionalProperties": false,
      "description": "The action requested by the user."
    },
    "sender": {
      "$ref": "common/user.schema.json"
    }
  },
  "required": [
    "action",
    "check_run",
    "repository",
    "sender"
  ],
  "additionalProperties": false,
  "title": "check_run rerequested event"
}