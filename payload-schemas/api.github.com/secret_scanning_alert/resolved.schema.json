{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "secret_scanning_alert$resolved",
  "type": "object",
  "properties": {
    "action": {
      "type": "string",
      "enum": [
        "resolved"
      ]
    },
    "alert": {
      "type": "object",
      "properties": {
        "number": {
          "type": "integer"
        },
        "resolution": {
          "type": [
            "string",
            "null"
          ],
          "enum": [
            "false_positive",
            "wontfix",
            "revoked",
            "used_in_tests"
          ]
        },
        "resolved_at": {
          "type": [
            "string",
            "null"
          ]
        },
        "resolved_by": {
          "$ref": "common/user.schema.json"
        },
        "secret_type": {
          "type": "string"
        }
      },
      "required": [
        "number",
        "secret_type",
        "resolution",
        "resolved_by",
        "resolved_at"
      ],
      "additionalProperties": false,
      "description": "The secret scanning alert involved in the event."
    },
    "installation": {
      "$ref": "common/installation-lite.schema.json"
    },
    "organization": {
      "$ref": "common/organization.schema.json"
    },
    "repository": {
      "$ref": "common/repository.schema.json"
    },
    "sender": {
      "$ref": "common/user.schema.json"
    }
  },
  "required": [
    "action",
    "alert",
    "repository"
  ],
  "additionalProperties": false,
  "title": "secret_scanning_alert resolved event"
}