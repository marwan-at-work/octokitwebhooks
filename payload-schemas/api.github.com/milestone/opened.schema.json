{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "milestone$opened",
  "type": "object",
  "properties": {
    "action": {
      "type": "string",
      "enum": [
        "opened"
      ]
    },
    "installation": {
      "$ref": "common/installation-lite.schema.json"
    },
    "milestone": {
      "allOf": [
        {
          "$ref": "common/milestone.schema.json"
        },
        {
          "type": "object",
          "properties": {
            "closed_at": {
              "type": "null"
            },
            "closed_issues": {
              "type": "integer"
            },
            "created_at": {
              "type": "string"
            },
            "creator": {
              "type": "object",
              "properties": {
                "avatar_url": {
                  "type": "string"
                },
                "events_url": {
                  "type": "string"
                },
                "followers_url": {
                  "type": "string"
                },
                "following_url": {
                  "type": "string"
                },
                "gists_url": {
                  "type": "string"
                },
                "gravatar_id": {
                  "type": "string"
                },
                "html_url": {
                  "type": "string"
                },
                "id": {
                  "type": "integer"
                },
                "login": {
                  "type": "string"
                },
                "node_id": {
                  "type": "string"
                },
                "organizations_url": {
                  "type": "string"
                },
                "received_events_url": {
                  "type": "string"
                },
                "repos_url": {
                  "type": "string"
                },
                "site_admin": {
                  "type": "boolean"
                },
                "starred_url": {
                  "type": "string"
                },
                "subscriptions_url": {
                  "type": "string"
                },
                "type": {
                  "type": "string"
                },
                "url": {
                  "type": "string"
                }
              }
            },
            "description": {
              "type": "string"
            },
            "due_on": {
              "type": "null"
            },
            "html_url": {
              "type": "string"
            },
            "id": {
              "type": "integer"
            },
            "labels_url": {
              "type": "string"
            },
            "node_id": {
              "type": "string"
            },
            "number": {
              "type": "integer"
            },
            "open_issues": {
              "type": "integer"
            },
            "state": {
              "type": "string",
              "enum": [
                "open"
              ]
            },
            "title": {
              "type": "string"
            },
            "updated_at": {
              "type": "string"
            },
            "url": {
              "type": "string"
            }
          },
          "required": [
            "state",
            "closed_at"
          ]
        }
      ]
    },
    "organization": {
      "$ref": "common/organization.schema.json"
    },
    "repository": {
      "$ref": "common/repository.schema.json"
    },
    "sender": {
      "$ref": "common/user.schema.json"
    }
  },
  "required": [
    "action",
    "milestone",
    "repository",
    "sender"
  ],
  "additionalProperties": false,
  "title": "milestone opened event"
}