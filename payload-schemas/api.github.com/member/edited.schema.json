{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "member$edited",
  "type": "object",
  "properties": {
    "action": {
      "type": "string",
      "enum": [
        "edited"
      ]
    },
    "changes": {
      "type": "object",
      "properties": {
        "old_permission": {
          "type": "object",
          "properties": {
            "from": {
              "type": "string",
              "description": "The previous permissions of the collaborator if the action was edited."
            }
          },
          "required": [
            "from"
          ],
          "additionalProperties": false
        },
        "permission": {
          "type": "object"
        }
      },
      "additionalProperties": false,
      "description": "The changes to the collaborator permissions"
    },
    "installation": {
      "$ref": "common/installation-lite.schema.json"
    },
    "member": {
      "$ref": "common/user.schema.json",
      "description": "The user who's permissions are changed."
    },
    "organization": {
      "$ref": "common/organization.schema.json"
    },
    "repository": {
      "$ref": "common/repository.schema.json"
    },
    "sender": {
      "$ref": "common/user.schema.json"
    }
  },
  "required": [
    "action",
    "changes",
    "member",
    "repository",
    "sender"
  ],
  "additionalProperties": false,
  "title": "member edited event"
}